{
  "functions": { "source": "functions" },
  "hosting": [{
    "source": "apps/web",
    "frameworksBackend": { "region": "us-central1" },
    "headers": [{
      "source": "**/*",
      "headers": [{ "key": "X-Frame-Options", "value": "ALLOWALL" }]
    }]
  }],
  "firestore": { "rules": "firestore.rules" },
  "storage": { "rules": "storage.rules" }
}
